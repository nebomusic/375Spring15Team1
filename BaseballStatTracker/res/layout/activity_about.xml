<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="0.1dp"
    android:paddingLeft="0.1dp"
    android:paddingRight="0.1dp"
    android:paddingTop="0.1dp"
    tools:context="com.example.baseballstattracker.About" >

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#CCCCCC" >

        <Button
            android:id="@+id/buttonStartGameBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="handleClick"
            android:text="Back"
            android:background="#00000000" />

    </TableRow>

    <TextView
        android:id="@+id/textAB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Created by Devin Summers and John Hammer" />

</LinearLayout>
